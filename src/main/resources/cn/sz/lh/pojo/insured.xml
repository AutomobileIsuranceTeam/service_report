<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.sz.lh.dao.IInsuredDao">

<select id="selectbyidcard" parameterType="cn.sz.lh.pojo.Insured" resultType="cn.sz.lh.pojo.Insured">
select insid,name,idcard,address from insured where idcard=#{idcard}
</select>


</mapper>